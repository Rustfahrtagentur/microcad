//   a(2.0)
//     D
//   a(3.0)
//     D
//__builtin::geo2d::Circle(radius = 42.0).a([2.0, 3.0]).b();



// c(2.0)
//    b
//       a(2.0)
//          D
//__builtin::geo2d::Circle(radius = 42.0).c([4.0, 5.0]);

