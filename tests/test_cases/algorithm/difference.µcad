use std::*;


export("{OUTPUT_FILE}.stl") algorithm::difference() {
    geo3d::sphere(radius = 3.0mm);
    geo3d::cube(size_x = 3.0mm, size_y = 3.0mm, size_z = 3.0mm);
}


export("{OUTPUT_FILE}.svg") algorithm::difference() {
    geo2d::circle(radius = 3.0mm);
    geo2d::rect(width = 3.0mm, height = 2.0mm);
}
