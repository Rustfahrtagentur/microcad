pub use __builtin::print;
pub use __builtin::ops;
pub use __builtin::math;

mod debug {
    pub use __builtin::assert;
    pub use __builtin::assert_eq;
    pub use __builtin::assert_invalid;
    pub use __builtin::assert_valid;
    pub use __builtin::type_of;
}

pub use transform::translate;
pub use transform::rotate;

/// Export helper part.
part export(filename: String) {
    #[export(filename)] {
        @children
    }
}

pub use __builtin::print;
pub use __builtin::error;
pub use __builtin::warning;
pub use __builtin::info;